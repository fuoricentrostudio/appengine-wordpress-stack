{
  "name": "fuoricentrostudio/piananotizie2014",
  "type": "project",
  "license": "MIT",
  "homepage": "https://www.piananotizie.it/",
  "authors": [
    {
      "name": "Brando Meniconi",
      "email": "b.meniconi@fuoricentrostudio.com",
      "homepage": "https://github.com/brandomeniconi"
    }
  ],
  "keywords": [
    "appengine", "wordpress", "stack", "php", "composer",  "wp", "bedrock", "roots"
  ],
  "config": {
    "preferred-install": "dist",
    "generate-salts": true
  },
  "autoload": {
    "psr-0": {"Roots\\Bedrock\\Installer": "scripts"}
  },
  "scripts": {
    "post-root-package-install": ["Roots\\Bedrock\\Installer::addSalts"]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/installers": "~1.0.12",
    "johnpbloch/wordpress": "4.2.1",
    "wpackagist-plugin/google-app-engine": ">=1.6",
    "wpackagist-plugin/batcache": "*"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
