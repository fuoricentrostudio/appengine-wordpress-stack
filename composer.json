{
  "name": "fuoricentrostudio/piananotizie",
  "type": "project",
  "license": "MIT",
  "homepage": "https://www.piananotizie.it/",
  "authors": [
    {
      "name": "Brando Meniconi",
      "email": "b.meniconi@fuoricentrostudio.com",
      "homepage": "https://github.com/brandomeniconi"
    }
  ],
  "keywords": [
    "appengine", "wordpress", "stack", "php", "composer",  "wp", "bedrock", "roots"
  ],
  "config": {
    "preferred-install": "dist",
    "generate-salts": true
  },
  "autoload": {
    "psr-0": {"Roots\\Bedrock\\Installer": "scripts"}
  },
  "scripts": {
    "post-root-package-install": ["Roots\\Bedrock\\Installer::addSalts"],
    "deploy":"appcfg.py --oauth2 update ."
  },
  "repositories": [
    {
        "type": "vcs",
        "url":  "git@bitbucket.org:fuoricentrostudio/piananotizie2014.git"
    },      
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/installers": "~1.0.12",
    "johnpbloch/wordpress": "4.2.2",
    "wpackagist-plugin/google-app-engine": ">=1.6",
    "wpackagist-plugin/batcache": "*",
    "wpackagist-plugin/advanced-custom-fields": "*",
    "wpackagist-plugin/all-in-one-seo-pack": "*",
    "fuoricentrostudio/piananotizie2014": "dev-master"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
